using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace Triangle64
{
    public class Triangle
    {
        public int ischeck(int x, int y,int z)
        {
if(x==y && y==z && z==x)
return 1;
if(x==y || y==z || z==x)
return 2;
else
      return 3;
        }
    }
}

3.	 Add new class Library

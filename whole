using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace Prime64
{
    public class Prime
    {
        public int isPrime(int n)
  {
		int c=0;
		for(int i=1;i<n;i=i+1)
		{
			if(n%i==0)
			c=c+1;
			if(c>1)
			return 0;
		}
		if(c==1)
		return 1;
		return 0;
} }}

